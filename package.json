{
  "name": "typatone",
  "displayName": "typatone",
  "description": "A Visual Studio Code extension that creates sweet melodies based on what you type. Inspired by typatone.com",
  "version": "0.0.1",
  "publisher": "smontlouis",
  "author": {
    "name": "St√©phane Montlouis-Calixte"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "keywords": [
    "sound",
    "voice",
    "audio",
    "keyboard",
    "fun",
    "typatone"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typatone.enable",
        "title": "Typatone: Enable"
      },
      {
        "command": "typatone.disable",
        "title": "Typatone: Disable"
      },
      {
        "command": "typatone.changeTheme",
        "title": "Typatone: Change theme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "eslint-config-prettier": "^8.5.0"
  },
  "dependencies": {
    "play-sound": "^1.1.5"
  }
}
